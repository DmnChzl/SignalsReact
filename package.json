{
  "name": "@dmnchzl/signals-react",
  "version": "0.0.3",
  "description": "A proposal to add Signals to React...",
  "author": "Damien Chazoule <dmnchzl@pm.me>",
  "repository": "https://github.com/dmnchzl/signalsreact.git",
  "type": "module",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --ui",
    "lint": "eslint src",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write"
  },
  "dependencies": {
    "alien-signals": "~2.0.5",
    "react": "~19.1.0",
    "react-dom": "~19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "~9.27.0",
    "@testing-library/jest-dom": "~6.6.0",
    "@testing-library/react": "~16.3.0",
    "@types/node": "~22.15.20",
    "@types/react": "~19.1.5",
    "@types/react-dom": "~19.1.5",
    "@vitejs/plugin-react-swc": "~3.9.0",
    "@vitest/ui": "~3.1.0",
    "eslint": "~9.27.0",
    "eslint-plugin-prettier": "~5.4.0",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-react-refresh": "~0.4.20",
    "eslint-plugin-testing-library": "~7.2.0",
    "globals": "~16.1.0",
    "happy-dom": "~17.4.5",
    "prettier": "~3.5.0",
    "typescript": "~5.8.0",
    "typescript-eslint": "~8.32.0",
    "vite": "~6.3.5",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "~3.1.0"
  },
  "engines": {
    "node": ">=18.20.0"
  },
  "license": "WTFPL"
}
